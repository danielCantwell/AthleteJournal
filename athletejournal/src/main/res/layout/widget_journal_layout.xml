<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:text="Calories"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#888888" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/textView2"
        android:gravity="center"
        android:text="Fat"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#888888" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/textView5"
        android:gravity="center"
        android:text="Carbs"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#888888" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/textView4"
        android:gravity="center"
        android:text="Protein"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#888888" />

    <com.cantwellcode.athletejournal.SmallDecimalTextView
        android:id="@+id/n_cal_text"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:text="0"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <com.cantwellcode.athletejournal.SmallDecimalTextView
        android:id="@+id/n_fat_text"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/n_cal_text"
        android:gravity="center"
        android:text="0"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <com.cantwellcode.athletejournal.SmallDecimalTextView
        android:id="@+id/n_carbs_text"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView4"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/n_fat_text"
        android:gravity="center"
        android:text="0"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <com.cantwellcode.athletejournal.SmallDecimalTextView
        android:id="@+id/n_protein_text"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView3"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/n_carbs_text"
        android:gravity="center"
        android:text="0"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/n_cal_desired_text"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/n_cal_text"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:text="2400"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#888888" />

    <TextView
        android:id="@+id/n_fat_desired_text"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/n_fat_text"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/n_cal_desired_text"
        android:gravity="center"
        android:text="50"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#888888" />

    <TextView
        android:id="@+id/n_carbs_desired_text"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/n_carbs_text"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/n_fat_desired_text"
        android:gravity="center"
        android:text="400"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#888888" />

    <TextView
        android:id="@+id/n_protein_desired_text"
        android:layout_width="66dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/n_protein_text"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@id/n_carbs_desired_text"
        android:gravity="center"
        android:text="100"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#888888" />

</RelativeLayout>